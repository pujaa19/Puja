#include<stdio.h>
int main(){
int arr[100],i,n;
 printf("The size of array:");
   scanf("%d",&n);
for(i=0;i<n;i++){
printf("The index of array :%d\n",i);
scanf("%d",&arr[i]);
}
printf("The values store into the array:\n");
for(i=0;i<n;i++){
printf("%d ",arr[i]);
}
printf("\n");
printf("The reverse values of array:");
for(i=n-1;i>=0;i--){
printf("%d",arr[i]);
}
return 0;
}
